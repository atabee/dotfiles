# Local Git Configuration
# This file contains user-specific settings that should NOT be tracked in git
# Copy this file to ~/.dotfiles/git/.gitconfig.local and customize with your information

[user]
	name = Your Name
	email = your.email@example.com

[credential]
	# macOS: use osxkeychain
	# Linux: use cache or store
	helper = osxkeychain

# Add any other local customizations here
